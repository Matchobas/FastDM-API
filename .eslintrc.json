{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:@typescript-eslint/recommended",
        ""
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "eslint-plugin-import-helpers"
    ],
    "rules": {
      "camelcase": "off",
      "import/no-unresolved": "error",
      "max-len": ["error", { "code": 120 }],
      "class-methods-use-this": "off"
    },
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    }
}
